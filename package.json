{
  "name": "front-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start --trace-deprecation",
    "build": "node --max-old-space-size=4096 node_modules/react-scripts/scripts/build.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --fix ",
    "prepare": "husky install",
    "tests-e2e": "unset ELECTRON_RUN_AS_NODE && npx cypress run --record --key 444a0b80-7bbd-4f3e-9dae-2c95d220c85d --browser chrome --headless",
    "cypress:open": "cypress open",
    "dependency-violations": "npx depcruise src",
    "update-packages": "npx npm-check-updates --color --interactive &&  npm cache clean --force && rm -Rf node_modules/ && npm install && npx npm-check"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/joy": "^5.0.0-beta.21",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.7",
    "@react-oauth/google": "^0.12.1",
    "@types/node": "^20.10.8",
    "@types/react-dom": "^18.2.18",
    "@types/react-notifications-component": "^3.1.4",
    "axios": "^1.6.5",
    "bootstrap": "^5.3.2",
    "echarts": "^5.4.3",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "pdfmake": "^0.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "sweetalert2": "^11.10.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@faker-js/faker": "^8.3.1",
    "@testing-library/react": "^14.1.2",
    "@types/pdfmake": "^0.2.8",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^13.6.2",
    "dependency-cruiser": "^16.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "i": "^0.3.7",
    "npm": "^10.2.5",
    "sass": "^1.69.7",
    "typescript": "^4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

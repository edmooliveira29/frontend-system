{
  "name": "front-end",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start --heapsnapshot-signal=SIGUSR2 --inspect",
    "build": "node --trace-gc --max-old-space-size=4096 node_modules/react-scripts/scripts/build.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --fix ",
    "prepare": "husky install",
    "tests-e2e": "unset ELECTRON_RUN_AS_NODE && cypress run",
    "dependency-violations": "npx depcruise src",
    "update-packages": "npx npm-check-updates --color --interactive &&  npm cache clean --force && rm -Rf node_modules/ && npm install && npx npm-check"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.8",
    "@mui/joy": "^5.0.0-beta.5",
    "@mui/lab": "^5.0.0-alpha.143",
    "@mui/material": "^5.14.8",
    "@mui/x-date-pickers": "^6.13.0",
    "@react-oauth/google": "^0.11.1",
    "@types/node": "^20.6.0",
    "@types/react-dom": "^18.2.7",
    "@types/react-notifications-component": "^3.1.1",
    "axios": "^1.5.0",
    "bootstrap": "^5.3.1",
    "echarts": "^5.4.3",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "pdfmake": "^0.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "sweetalert2": "^11.7.27"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@faker-js/faker": "^8.0.2",
    "@testing-library/react": "^14.0.0",
    "@types/pdfmake": "^0.2.2",
    "@types/redux-logger": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^13.1.0",
    "dependency-cruiser": "^13.1.5",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "i": "^0.3.7",
    "npm": "^10.2.0",
    "sass": "^1.66.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
